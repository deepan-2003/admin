[32m+[m[32mconst events = [[m
[32m+[m[32m  { date: "2025-03-01", title: "National Day", type: "Holiday", color: "pink.200" },[m
[32m+[m[32m  { date: "2025-03-10", title: "Summer Holiday Event", type: "Event", color: "blue.200" },[m
